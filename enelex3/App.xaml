<Application x:Class="enelex3.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:enelex3"
             xmlns:start="clr-namespace:enelex3.Windows"
              >
    <Application.Resources>        
            <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Resources/Colours.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Resources/Brushes.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Resources/GeometryDictionary.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Resources/PathStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Localization/LocalizationRS-LAT.xaml"></ResourceDictionary>
                <ResourceDictionary Source="pack://application:,,,/Resources/Style.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            </ResourceDictionary>      
    </Application.Resources>
</Application>
