    <Window x:Class="enelex3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:enelex3"
        mc:Ignorable="d"
       Title="Enelex GE3030" Icon="Pics/nuclear.ico" Width="1455" Height="800" WindowStartupLocation="CenterScreen" WindowState="Maximized"  Background="WhiteSmoke">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="auto"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <StackPanel Orientation="Horizontal" Margin="2,2,0,-2" >
            <Button Style="{DynamicResource Load}" BorderBrush="Transparent" Background="Transparent" Click="Load_Click"></Button>
            <Button Style="{DynamicResource Add}" Visibility="Collapsed"  BorderBrush="Transparent" Background="Transparent" Click="Add_Click_1"></Button>
            <Button Style="{DynamicResource Update}" BorderBrush="Transparent" Background="Transparent" Click="Button_Click"></Button>
            <Button Style="{DynamicResource Delete}" BorderBrush="Transparent" Background="Transparent" Click="Button_Click_1"></Button>
            <Button Style="{DynamicResource Excel}" BorderBrush="Transparent" Background="Transparent" VerticalAlignment="Top" Click="Excel_Button"></Button>
            
        </StackPanel>
        <StackPanel Grid.Row="1" Background="White" Orientation="Horizontal" Margin="5,5,0,0">
            <Label Margin="-3,0,0,0">Br</Label>
            <TextBox Name="tbBr" Width="42"  Height="20" ></TextBox>
            <Label Margin="-3,0,0,0">GE</Label>
            <TextBox Name="tbGE" Width="42"  Height="20" Text="{Binding NewMeasureGE, Mode=TwoWay, StringFormat={}{0:F2}}"></TextBox>
            <Label Margin="-2,0,0,0">LAB</Label>
            <TextBox Name="tbLAB" Width="42" Height="20"  Text="{Binding NewMeasureLAB, Mode=TwoWay, StringFormat={}{0:F2}}"></TextBox>
            <Label Margin="-2,0,0,0">W</Label>
            <TextBox Name="tbW" Width="42" Height="20"  Text="{Binding NewMeasureW, Mode=TwoWay, StringFormat={}{0:F2}}"></TextBox>
            <Button Click="Button_Click_3" Style="{DynamicResource Add2}"></Button>
        </StackPanel>
        <DataGrid Name="dgMeasures"    Grid.Row="2" Grid.RowSpan="3" Style="{StaticResource dgCustom}"    ItemsSource="{Binding ListOfMeasures}" SelectionChanged="dgMeasures_SelectionChanged" LoadingRow="DgMeasures_LoadingRow">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Broj n [ j ] " Binding="{Binding IdSort}"></DataGridTextColumn>
                <DataGridTextColumn Header="Pepeo x [GE] " Binding="{Binding Ge,  StringFormat={}{0:F2}}"  ></DataGridTextColumn>
                <DataGridTextColumn Header="Pepeo y [Lab]" Binding="{Binding Lab, StringFormat={}{0:F2}}"  ></DataGridTextColumn>
                <DataGridTextColumn Header="Vlaga [W]" Binding="{Binding W, StringFormat={}{0:F2}}" Width="87" ></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
        <StackPanel Grid.Row="1" Grid.Column="1" Grid.RowSpan="32" Background="White" Orientation="Vertical" Margin="0,5,0,0">
            <GroupBox Header="Izračunavanje p i q" Margin="5,5,5,0" BorderThickness="2">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                    </Grid.RowDefinitions>
                    <StackPanel Orientation="Horizontal" Grid.Row="0" Margin="10,10,0,0">
                        <Image Source="Pics/suma1.png" Width="80" Margin="0,-10,0,0"></Image>
                        <Label Margin="0,5,0,0">=</Label>
                        <TextBox Name="tbSum1" Width="80" VerticalContentAlignment="Center" Margin="0,10,0,27" Height="20"></TextBox>
                        <Image Source="Pics/suma2.png" Width="45" Margin="5,-10,0,0"></Image>
                        <Label Margin="0,5,0,0">=</Label>
                        <TextBox Name="tbSum2" Width="80" VerticalContentAlignment="Center" Margin="0,10,0,27" Height="20"></TextBox>
                        <Image Source="Pics/suma3.png" Width="45" Margin="5,-10,0,0"></Image>
                        <Label Margin="0,5,0,0">=</Label>
                        <TextBox Name="tbSum3" Width="80" VerticalContentAlignment="Center" Margin="0,10,0,27" Height="20"></TextBox>
                        <Image Source="Pics/suma4.png" Width="55" Margin="5,-10,0,0"></Image>
                        <Label Margin="0,5,0,0">=</Label>
                        <TextBox Name="tbSum4" Width="80" VerticalContentAlignment="Center" Margin="0,10,0,27" Height="20"></TextBox>
                        <Image Source="Pics/suma5.png" Width="75" Margin="5,-10,0,0"></Image>
                        <Label Margin="0,5,0,0">=</Label>
                        <TextBox Name="tbSum5" Width="80" VerticalContentAlignment="Center" Margin="0,10,0,27" Height="20"></TextBox>
                        <Label Margin="0,5,0,0">n=</Label>
                        <TextBox Name="tbn" Width="80" VerticalContentAlignment="Center" Margin="0,10,0,27" Height="20"></TextBox>
                    </StackPanel>
                    <StackPanel  Grid.Row="1" Orientation="Horizontal" Margin="0,-15,0,0">
                        <StackPanel Margin="10,10,0,0">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto" ></ColumnDefinition>
                                    <ColumnDefinition ></ColumnDefinition>
                                    <ColumnDefinition ></ColumnDefinition>
                                    <ColumnDefinition ></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Image Grid.Column="1"  Source="Pics/p.png" Width="250"></Image>
                                <Label Grid.Column="2" Margin="0,50,0,0">=</Label>
                                <TextBox Name="tbP"  Grid.Column="3" Height="20"  Width="100" HorizontalAlignment="Left" Background="#ffffd8" VerticalContentAlignment="Center" Margin="0,55,0,57" Text="{Binding SumP, Mode=TwoWay, StringFormat=N4}"></TextBox>
                            </Grid>
                        </StackPanel>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Grid.Row="2" Margin="10,5,0,0">
                        <Image Source="Pics/suma3.png" Width="42" Margin="0,-10,0,0"></Image>
                        <Label Margin="0,5,0,0">=</Label>
                        <TextBox x:Name="tbSumQ1" Width="80" VerticalContentAlignment="Center" Margin="0,10,0,28" Height="20"/>
                        <Image Source="Pics/suma6.png" Width="45" Margin="5,-10,0,0"></Image>
                        <Label Margin="0,5,0,0">=</Label>
                        <TextBox Name="tbSumQ2" Width="80" VerticalContentAlignment="Center" Margin="0,10,0,28" Height="20"></TextBox>
                        <Label Margin="0,5,0,0">n=</Label>
                        <TextBox Name="tbn1" Width="80" VerticalContentAlignment="Center" Margin="0,10,0,28" Height="20"></TextBox>
                    </StackPanel>
                    <StackPanel Margin="10,-20,0,0" Grid.Row="3" Orientation="Horizontal">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" ></ColumnDefinition>
                                <ColumnDefinition  ></ColumnDefinition>
                                <ColumnDefinition ></ColumnDefinition>
                                <ColumnDefinition ></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Image Grid.Column="1" Source="Pics/q.png" Width="155" Margin="0,5,0,0"></Image>
                            <Label Grid.Column="2" Margin="0,67,0,0">=</Label>
                            <TextBox Name="tbQ" Grid.Column="3" Width="100" HorizontalAlignment="Left" Background="#ffffd8" VerticalContentAlignment="Center" Margin="0,72,0,37" Text="{Binding SumQ, Mode=TwoWay, StringFormat=N4}"></TextBox>
                        </Grid>
                    </StackPanel>
                </Grid>
            </GroupBox>
            <StackPanel Margin="0,-10,5,0">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition></ColumnDefinition>
                        <ColumnDefinition></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <GroupBox Header="Novi tip kalibracije" Margin="5,5,5,0" BorderThickness="2" >
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"></ColumnDefinition>
                                <ColumnDefinition Width="auto"></ColumnDefinition>
                                <ColumnDefinition Width="auto"></ColumnDefinition>
                                <ColumnDefinition ></ColumnDefinition>
                                <ColumnDefinition Width="auto"></ColumnDefinition>
                                
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto"></RowDefinition>
                                <RowDefinition Height="25"></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition Height="auto"></RowDefinition>
                            </Grid.RowDefinitions>
                            <StackPanel Orientation="Horizontal" Margin="0,5,0,12">
                                <Button Style="{DynamicResource SaveButton}" Click="Save_Click"  Margin="5,0,0,0" ></Button>
                                <Button Style="{DynamicResource UpdateButton}" Click="Button_Click_4"></Button>
                            </StackPanel >
                            <StackPanel Grid.Column="4" Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="Right">
                                <Button  Style="{DynamicResource Graph}" Click="Graph1" Height="50" Margin="5,0,0,0" ></Button>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Grid.Row="1" Height="20">
                                <TextBox Width="72" Height="20" Margin="5,0,5,0"  Text="{Binding NumberAp, Mode=TwoWay, StringFormat={}{0:F2}}"  ></TextBox>
                                <TextBox Width="72" Height="20" Text="{Binding NumberBp, Mode=TwoWay, StringFormat={}{0:F2}}" ></TextBox>
                            </StackPanel>
                            <DataGrid Name="dgTipAB" Style="{StaticResource dgCustom }" ItemsSource="{Binding ListOfCalibrations}"  CanUserAddRows="False"  Grid.Row="2" Height="45" Margin="5,0,0,7" >
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="Vrednost [a]" Binding="{Binding NumberA, Mode=TwoWay, StringFormat={}{0:F2}}" ></DataGridTextColumn>
                                    <DataGridTextColumn Header="Vrednost [b]" Binding="{Binding NumberB, Mode=TwoWay, StringFormat={}{0:F2}}" ></DataGridTextColumn>
                                </DataGrid.Columns>
                            </DataGrid>
                            <StackPanel Margin="5,0,0,0" Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="2" >
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="auto" ></ColumnDefinition>
                                        <ColumnDefinition ></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition ></RowDefinition>
                                        <RowDefinition ></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Label Margin="-2,0,0,0">A = p * a =</Label>
                                    <TextBox Name="tbA" Grid.Column="1" Width="70" HorizontalAlignment="Left" Background="#ffffd8" VerticalContentAlignment="Center" Height="20" ></TextBox>
                                </Grid>
                            </StackPanel>
                            <StackPanel Grid.Column="1" Grid.Row="2" Grid.ColumnSpan="2">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="auto" ></ColumnDefinition>
                                        <ColumnDefinition ></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <Label Margin="3,0,0,0">B = (p * b) + q =</Label>
                                    <TextBox Name="tbB" Grid.Column="1" Width="70" HorizontalAlignment="Left" Background="#ffffd8" VerticalContentAlignment="Center" Height="20"></TextBox>
                                </Grid>
                            </StackPanel>
                            <StackPanel Grid.Column="3" Grid.Row="1" Orientation="Horizontal">
                                <Label>P=</Label>
                                <TextBox Width="70" Background="#ffffd8" Height="20" Margin="0,0,0,0" ></TextBox>
                            </StackPanel>
                            <StackPanel Grid.Column="3" Grid.Row="2" Orientation="Horizontal">
                                <Label>Q=</Label>
                                <TextBox Width="70" Background="#ffffd8" Height="20" Margin="0,-25,0,0" ></TextBox>
                            </StackPanel>
                            <StackPanel Grid.Column="1" Grid.Row="3" Orientation="Horizontal">
                                <Label Margin="3,-30,0,0">R=</Label>
                                <TextBox Width="70" Background="#ffffd8" Margin="0,-30,0,0" Height="20" ></TextBox>
                            </StackPanel>
                            <StackPanel Grid.Column="3" Grid.Row="4" Orientation="Horizontal">
                                <Label Margin="0,-28,0,0">W=</Label>
                                <TextBox Name="tbV" Width="70" Background="#ffffd8" Height="20" Margin="0,-28,0,0"  ></TextBox>
                            </StackPanel>
                        </Grid>
                    </GroupBox>
                    <GroupBox Header="Apsolutno pomeranje" Grid.Column="1" Margin="5,5,0,0" BorderThickness="2">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto"></RowDefinition>
                                <RowDefinition Height="25"></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"></ColumnDefinition>
                                <ColumnDefinition Width="auto"></ColumnDefinition>
                                <ColumnDefinition Width="auto"></ColumnDefinition>
                                <ColumnDefinition Width="auto"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <StackPanel Orientation="Horizontal" Margin="0,0,0,0" >
                                <Button Style="{DynamicResource SaveButton}" Click="Button_Click_6" Margin="5,2,0,16"></Button>
                                <Button Style="{DynamicResource UpdateButton}" Margin="5,2,0,16" Click="Button_Click_4"></Button>
                            </StackPanel >
                            <StackPanel Orientation="Horizontal" Grid.Row="1" Height="20" Margin="0,0,0,0">
                                <TextBox Width="72" Height="20" Margin="5,0,5,0"  Text="{Binding NumberPr, Mode=TwoWay, StringFormat={}{0:F2}}"  ></TextBox>
                                
                            </StackPanel>
                            <StackPanel  Grid.Row="2" Grid.Column="0" Margin="0,0,0,0">
                                <DataGrid Name="dgApsolutno" Style="{StaticResource dgCustom}" ItemsSource="{Binding ListOfPercetange}" Height="45" CanUserAddRows="False"  >
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Procenat pomeranja [k]" Binding="{Binding NumberP,  Mode=TwoWay, StringFormat={}{0:F2}}"></DataGridTextColumn>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </StackPanel>
                            
                            <StackPanel Grid.Column="1" Margin="0,5,0,10">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="auto"></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <StackPanel Orientation="Horizontal" >
                                        <Button Style="{DynamicResource SaveButton}" Click="Button_Click_7" Margin="0,-3,0,0"></Button>
                                        <Button Style="{DynamicResource UpdateButton}" Margin="0,-3,0,0" Click="Button_Click_4"></Button>
                                    </StackPanel >
                                </Grid>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Grid.Row="1" Grid.Column="1" Height="20">
                                <TextBox Width="72" Height="20" Margin="5,0,5,0"  Text="{Binding NumberAa, Mode=TwoWay, StringFormat={}{0:F2}}"  ></TextBox>
                                <TextBox Width="72" Height="20" Text="{Binding NumberBa, Mode=TwoWay, StringFormat={}{0:F2}}" ></TextBox>
                            </StackPanel>
                            <StackPanel Grid.Row="2" Grid.Column="1">
                                <DataGrid Name="dgTipAB2"   Style="{StaticResource dgCustom}" ItemsSource="{Binding ListOfCalibrationTwo}" Height="45" CanUserAddRows="False"   >
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Vrednost [a]" Binding="{Binding NumberATwo, Mode=TwoWay, StringFormat={}{0:F2}}" ></DataGridTextColumn>
                                        <DataGridTextColumn Header="Vrednost [b]" Binding="{Binding NumberBTwo, Mode=TwoWay, StringFormat={}{0:F2}}" ></DataGridTextColumn>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </StackPanel>
                            
                            <StackPanel Grid.Column="2" Orientation="Vertical">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                    </Grid.RowDefinitions>
                                </Grid>
                                <StackPanel Orientation="Horizontal" Grid.Row="0" Grid.Column="2">
                                    <Image Source="Pics/A2.png" Width="180" Margin="12,0,0,0"></Image>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Grid.Column="2" Grid.Row="0" Margin="10,40,0,0" Grid.RowSpan="2" >
                                <Label Margin="0,-10,0,20">A =</Label>
                                <TextBox Name="tbAprocenat" Width="70" Height="20" HorizontalAlignment="Left" Background="#ffffd8" VerticalContentAlignment="Center" Margin="0,-25,0,10"  ></TextBox>
                            </StackPanel>
                            <StackPanel Grid.Row="1" Grid.Column="2" Orientation="Horizontal" Margin="10,15,0,-20" >
                                <Label Margin="0,5,0,0">B =</Label>
                                <TextBox Name="tbBprocenat" Width="70" Height="20" HorizontalAlignment="Left" Background="#ffffd8" VerticalContentAlignment="Center" Margin="0,5,0,0" ></TextBox>
                            </StackPanel>
                            <StackPanel Grid.Column="4" Grid.Row="1" Grid.RowSpan="2" Orientation="Horizontal" HorizontalAlignment="Right">
                                <Button  Style="{DynamicResource Graph}" Click="Graph2" Height="50" Margin="5,23,0,0" ></Button>
                            </StackPanel>
                        </Grid>
                    </GroupBox>
                </Grid>
            </StackPanel>
            <StackPanel>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="auto"></ColumnDefinition>
                        <ColumnDefinition></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                    </Grid.RowDefinitions>
                    <GroupBox Header="Srazmerno pomeranje" Margin="5,-6,0,5" BorderThickness="2">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition ></RowDefinition>
                                <RowDefinition Height="25"></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"></ColumnDefinition>
                                <ColumnDefinition Width="auto"></ColumnDefinition>
                                <ColumnDefinition Width="auto"></ColumnDefinition>
                                <ColumnDefinition Width="auto"></ColumnDefinition>
                                <ColumnDefinition Width="auto"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <StackPanel Orientation="Horizontal" Margin="0,3,0,10" Grid.Column="0">
                                <Button Visibility="Collapsed" Style="{DynamicResource AddButton}" Click="Add2_Click"></Button>
                                <Button Style="{DynamicResource SaveButton}" Click="Button_Click_5" Margin="5,1,0,0"></Button>
                                <Button Style="{DynamicResource UpdateButton}" Margin="5,-1,0,0" Click="Update_One"></Button>
                                <Button Style="{DynamicResource DeleteButton}" Margin="5,2,0,0" Click="Del_One"></Button>
                            </StackPanel >
                            <StackPanel Orientation="Horizontal" Grid.Row="1" Height="20">
                                <TextBox Name="tbL" Width="82" Height="20" Margin="21,0,5,0"  Text="{Binding NumberL, Mode=TwoWay, StringFormat={}{0:F2}}"  ></TextBox>
                                <TextBox Width="82" Height="20" Margin="21,0,0,0"  Text="{Binding NumberP, Mode=TwoWay, StringFormat={}{0:F2}}" ></TextBox>
                            </StackPanel>
                            <DataGrid Name="dgOne" Style="{StaticResource dgCustom}" ItemsSource="{Binding ListOfCalibrationOne}" CanUserAddRows="False" Grid.Row="2" LoadingRow="DgOne_LoadingRow" Height="65" Margin="0,0,0,0" >
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="Br." Binding="{Binding ID}"></DataGridTextColumn>
                                    <DataGridTextColumn Header="Laboratorija L [%] " Binding="{Binding L, Mode=TwoWay, StringFormat={}{0:F2}}"  ></DataGridTextColumn>
                                    <DataGridTextColumn Header="Sadrzaj pepela P [%] " Binding="{Binding P, Mode=TwoWay, StringFormat={}{0:F2}}"></DataGridTextColumn>
                                    <DataGridTextColumn Header="Korigovanje k=L-P [%]  " Binding="{Binding SumLP}"></DataGridTextColumn>
                                </DataGrid.Columns>
                            </DataGrid>
                            <StackPanel Orientation="Horizontal" Margin="0,3,0,10" Grid.Column="1">
                                <Button Style="{DynamicResource SaveButton}" Click="Button_Click_8" ></Button>
                                <Button Style="{DynamicResource UpdateButton}" Click="Button_Click_4"></Button>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Grid.Row="1" Grid.Column="1" Height="20" Margin="5,0,0,0">
                                <TextBox Width="73" Height="20" Margin="5,0,5,0"  Text="{Binding NumberAs, Mode=TwoWay, StringFormat={}{0:F2}}"  ></TextBox>
                                <TextBox Width="73" Height="20" Text="{Binding NumberBs, Mode=TwoWay, StringFormat={}{0:F2}}" ></TextBox>
                            </StackPanel>
                            <StackPanel Grid.Column="1" Grid.Row="2" >
                                <DataGrid Name="dgTipAB3" Style="{StaticResource dgCustom}" ItemsSource="{Binding ListOfCalibarationsThree}" CanUserAddRows="False"  Margin="10,0,0,0">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Vrednost [a]" Binding="{Binding NumberAThree, Mode=TwoWay, StringFormat={}{0:F2}}" ></DataGridTextColumn>
                                        <DataGridTextColumn Header="Vrednost [b]" Binding="{Binding NumberBThree, Mode=TwoWay, StringFormat={}{0:F2}}" ></DataGridTextColumn>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </StackPanel>
                            <StackPanel Grid.Column="2" Grid.Row="1" Grid.RowSpan="2" Orientation="Vertical"  Margin="10,-10,0,0" >
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <StackPanel Orientation="Horizontal"  Margin="10,0,0,0">
                                        <Image Source="Pics/As.png" Width="95"></Image>
                                        <Label Margin="0,5,0,0">=</Label>
                                        <TextBox Name="tbAsraz" Width="70" Background="#ffffd8" VerticalContentAlignment="Center" Height="20" ></TextBox>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Grid.Row="1" Margin="13,0,0,0">
                                        <Image Source="Pics/Bs.png" Width="128"></Image>
                                        <Label Margin="0,5,0,0">=</Label>
                                        <TextBox Name="tbBsraz" Width="70" Background="#ffffd8" VerticalContentAlignment="Center" Height="20" Margin="0,0,0,0" Text="{Binding FinalB}"></TextBox>
                                    </StackPanel>
                                </Grid>
                            </StackPanel>
                            <StackPanel Grid.Column="3" Grid.Row="1" Grid.RowSpan="2" Orientation="Horizontal" Margin="0,0,0,0" HorizontalAlignment="Right">
                                <Button  Style="{DynamicResource Graph}" Click="Graph3"  Height="50" Margin="5,0,0,0" ></Button>
                            </StackPanel>
                        </Grid>
                    </GroupBox>
                </Grid>
            </StackPanel>
        </StackPanel>
        <StackPanel Grid.Row="3" Grid.Column="1"  >
            <StackPanel  Background="White"  HorizontalAlignment="Right">
                <Image Source="Pics/logo.png" Width="100" Margin="0,0,10,10"></Image>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
