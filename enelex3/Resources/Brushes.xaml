<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:enelex3.Resources">
    <SolidColorBrush x:Key="DefaultDetailsBrush" Color="{DynamicResource DefaultDetailColour}"></SolidColorBrush>
    <SolidColorBrush x:Key="DefaultBorderFirstColor" Color="{DynamicResource BorderFirstColour}"></SolidColorBrush>
    <SolidColorBrush x:Key="DefaultBorderSecondColor" Color="{DynamicResource BorderSecondColour}"></SolidColorBrush>
    <SolidColorBrush x:Key="DefaultBorderSaveColor" Color="{DynamicResource BorderSaveColour}"></SolidColorBrush>
</ResourceDictionary>